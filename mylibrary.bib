@article{butlerQuasiparticleInterferenceZrSiS2017,
  title = {Quasiparticle Interference in {{ZrSiS}}: {{Strongly}} Band-Selective Scattering Depending on Impurity Lattice Site},
  shorttitle = {Quasiparticle Interference in {{ZrSiS}}},
  author = {Butler, Christopher J. and Wu, Yu-Mi and Hsing, Cheng-Rong and Tseng, Yi and Sankar, Raman and Wei, Ching-Ming and Chou, Fang-Cheng and Lin, Minn-Tsong},
