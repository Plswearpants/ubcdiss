	pages = {414004},
	file = {IOP Full Text PDF:C\:\\Users\\ChenD\\Zotero\\storage\\YQR3S6ZR\\Macdonald et al. - 2016 - Dispersing artifacts in FT-STS a comparison of se.pdf:application/pdf},
}

