@article{avrahamQuasiparticleInterferenceStudies2018,
	title = {Quasiparticle {Interference} {Studies} of {Quantum} {Materials}},
	volume = {30},
	copyright = {© 2018 WILEY-VCH Verlag GmbH \& Co. KGaA, Weinheim},
