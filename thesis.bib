@article{aishwaryaMeltingChargeDensity2024,
  title = {Melting of the Charge Density Wave by Generation of Pairs of Topological Defects in {{UTe2}}},
  author = {Aishwarya, Anuva and May-Mann, Julian and Almoalem, Avior and Ran, Sheng and Saha, Shanta R. and Paglione, Johnpierre and Butch, Nicholas P. and Fradkin, Eduardo and Madhavan, Vidya},
  date = {2024-06},
  journaltitle = {Nature Physics},
